set ignorecase

let mapleader=","
"set clipboard=unnamed
"
"map <Space>qk :vsc Tools.CustomizeKeyboard<CR>
"map <Space>b :vsc Build.BuildSolution<CR>
"map <Space><Space> :vsc Tools.InvokeAceJumpCommand<CR>

nmap ]m :vsc Edit.NextMethod<CR>
nmap [m :vsc Edit.PreviousMethod<CR>

nmap <C-o> :vsc View.NavigateBackward<CR>
nmap <C-i> :vsc View.NavigateForward<CR>

map <Leader>nl :nohl<CR>
map <Leader>w :w<CR>
map <Leader>x :x<CR>
map <Leader>b :vsc Build.BuildSolution<CR>
map <Leader>r :vsc Debug.Start<CR>

map gcc :vsc Edit.ToggleLineComment<CR>
map gd :vsc Edit.GotoDefinition<CR>
map gD :vsc Edit.GotoDeclaration<CR>

vmap gc :vsc Edit.ToggleLineComment<CR>
